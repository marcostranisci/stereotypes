    <!-- http://stereotypes.di.unito.it/stereotypes.owl#ImmigrantGroup -->

    <owl:Class rdf:about="http://stereotypes.di.unito.it/stereotypes.owl#ImmigrantGroup">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://stereotypes.di.unito.it/stereotypes.owl#hasMember"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://xmlns.com/foaf/spec/#term_Person"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://stereotypes.di.unito.it/stereotypes.owl#hasAttribute"/>
                                <owl:allValuesFrom rdf:resource="http://stereotypes.di.unito.it/stereotypes.owl#NonCitizen"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://stereotypes.di.unito.it/stereotypes.owl#VulnerableGroup"/>
    </owl:Class>
    


